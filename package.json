  "scripts": {
    "build": "npm run build-blocked-appt-time && npm run build-common && npm run build-compiler && npm run build-core && npm run build-datetime && npm run build-forms && npm run build-sms",
    "build-blocked-appt-time": "cd modules/blocked-appt-time && bash build.sh && cd dist && npm pack && cp rd-blocked-appt-time-7.0.0.tgz ../../",
    "build-common": "cd modules/common && npm i && bash build.sh && cd dist && npm pack && cp rd-common-0.0.0-development.tgz ../../",
    "build-compiler": "cd modules/compiler && npm i && bash build.sh && cd dist && npm pack && cp rd-compiler-0.0.0-development.tgz ../../",
    "build-core": "cd modules/core && npm i && bash build.sh && cd dist && npm pack && cp rd-core-0.0.0-development.tgz ../../",
    "build-datetime": "cd modules/datetime && npm i && bash build.sh && cd dist && npm pack && cp rd-datetime-0.0.0-development.tgz ../../",
    "build-forms": "cd modules/forms && npm i && bash build.sh && cd dist && npm pack && cp rd-forms-0.0.0-development.tgz ../../",
    "build-sms": "cd modules/sms && bash build.sh && cd dist && npm pack && cp rd-sms-7.0.0.tgz ../../",
    "commit": "git-cz",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "postinstall": "ngc -p angular.tsconfig.json",
    "prepush": "ng test --single-run",
    "serve": "npm run build && lite-server -c=.bs-config.json",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test --single-run",
    "test-lib": "cd modules/angular-lib && bash test.sh"
  },
